{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build",
      "command": "dotnet",
      "type": "shell",
      "args": [
        "build",
        "${workspaceFolder}/Octonica.ClickHouseClient/Octonica.ClickHouseClient.csproj"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      }
    },
    {
      "label": "run",
      "command": "dotnet",
      "type": "shell",
      "args": [
        "run",
        "${workspaceFolder}/path/to/project.csproj"
      ],
      "group": {
        "kind": "test",
        "isDefault": true
      }
    },
    {
      "label": "test",
      "command": "dotnet",
      "type": "shell",
      "args": [
        "test",
        "${workspaceFolder}/path/to/project.csproj"
      ],
      "group": {
        "kind": "test",
        "isDefault": true
      }
    }
  ]
}
